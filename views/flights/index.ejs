<h1>All Flights</h1>
<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight No</th>
      <th>Departs</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(flight) { %>
      <tr>
        <td><%= flight.airline %></td>
        <td><%= flight.airport %></td>
        <td><%= flight.flightNo %></td>
        <td><%= flight.departs.toISOString().slice(0, 16).replace('T', ' ') %></td>
      </tr>
    <% }); %>
  </tbody>
</table>
